  "_notes": {
    "removed_dependencies": [
      "Removed 20+ unused @radix-ui components (accordion, alert-dialog, avatar, checkbox, etc.)",
      "Removed 3D libraries (@react-three/drei, @react-three/fiber, @react-three/postprocessing, three, leva)",
      "Removed animation libraries (framer-motion, react-spring, @use-gesture/react)",
      "Removed unused UI components (cmdk, embla-carousel-react, input-otp, next-themes, react-day-picker)",
      "Removed unused utilities (react-intersection-observer, react-resizable-panels, react-use, vaul)",
      "Removed testing libraries (playwright, @playwright/test, vitest, @vitest/ui, jsdom)",
      "Removed testing utilities (@testing-library/jest-dom, @testing-library/react, @testing-library/user-event)",
      "Removed sharp, @types/three, @types/estree"
    ],
    "kept_essentials": [
      "Core React, React Router",
      "Form handling (react-hook-form, @hookform/resolvers, zod)",
      "i18n (i18next, react-i18next)",
      "Data fetching (@tanstack/react-query, axios)",
      "Charts (recharts)",
      "Maps (leaflet, react-leaflet)",
      "UI primitives (essential @radix-ui components only)",
      "Icons (lucide-react)",
      "Styling (Tailwind, clsx)",
      "Toasts (sonner)"
    ]
  }
