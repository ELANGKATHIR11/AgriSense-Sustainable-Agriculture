# Production environment with ngrok tunnel
# Update VITE_BACKEND_API_URL and VITE_BACKEND_WS_URL after ngrok starts
# Example: https://1234-56-789-012-34.ngrok.io

VITE_POUCHDB_SERVER_URL=https://pouchdb.example.com
VITE_BACKEND_API_URL=https://YOUR_NGROK_URL/api/v1
VITE_BACKEND_WS_URL=wss://YOUR_NGROK_URL
VITE_ENABLE_OFFLINE_MODE=true
VITE_ENABLE_POUCHDB_SYNC=false
VITE_LOG_LEVEL=debug
