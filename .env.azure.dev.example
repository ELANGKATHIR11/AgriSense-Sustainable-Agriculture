# ===================================================================
# AgriSense Azure Environment Variables - Development
# ===================================================================

# Azure Configuration
AZURE_SUBSCRIPTION_ID=your-subscription-id-here
AZURE_TENANT_ID=your-tenant-id-here
AZURE_RESOURCE_GROUP=agrisense-dev-rg
AZURE_LOCATION=eastus

# Container Registry
AZURE_CONTAINER_REGISTRY=agrisensedevregistry
AZURE_ACR_USERNAME=agrisensedevregistry
AZURE_ACR_PASSWORD=<from-azure-portal>

# App Service
AZURE_BACKEND_APP_NAME=agrisense-dev-backend-xxxxx
AZURE_FRONTEND_URL=https://agrisense-dev-frontend.azurestaticapps.net

# Static Web App
AZURE_STATIC_WEB_APPS_API_TOKEN=<from-azure-portal>

# Database - Cosmos DB
COSMOS_DB_ENDPOINT=https://agrisense-dev-cosmos-xxxxx.documents.azure.com:443/
COSMOS_DB_KEY=<from-azure-portal>
COSMOS_DB_DATABASE_NAME=AgriSense

# Storage Account
AZURE_STORAGE_CONNECTION_STRING=DefaultEndpointsProtocol=https;AccountName=agrisensedevxxxxx;AccountKey=<key>;EndpointSuffix=core.windows.net
AZURE_STORAGE_ACCOUNT_NAME=agrisensedevxxxxx
AZURE_STORAGE_ACCOUNT_KEY=<from-azure-portal>

# Application Insights
APPLICATIONINSIGHTS_CONNECTION_STRING=InstrumentationKey=xxxxx;IngestionEndpoint=https://eastus-8.in.applicationinsights.azure.com/
APPINSIGHTS_INSTRUMENTATIONKEY=xxxxx

# Key Vault
KEY_VAULT_URL=https://agrisense-dev-kv-xxxxx.vault.azure.net/

# Backend Configuration
AGRISENSE_ENV=development
AGRISENSE_DISABLE_ML=0
PYTHON_VERSION=3.12.10
PORT=8004
WORKERS=2

# Frontend Configuration
VITE_API_URL=https://agrisense-dev-backend-xxxxx.azurewebsites.net
VITE_APP_VERSION=1.0.0
NODE_ENV=development

# Security
CORS_ORIGINS=https://agrisense-dev-frontend.azurestaticapps.net,http://localhost:8080
JWT_SECRET_KEY=<generate-secure-random-key>
AGRISENSE_ADMIN_TOKEN=<generate-secure-random-key>

# MQTT (if using IoT Hub)
MQTT_BROKER=agrisense-dev-iothub.azure-devices.net
MQTT_PORT=8883
MQTT_USERNAME=<from-iot-hub>
MQTT_PASSWORD=<from-iot-hub>

# Logging
LOG_LEVEL=INFO
ENABLE_DEBUG=true
